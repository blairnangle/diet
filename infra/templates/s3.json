{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "PublicReadGetObject",
      "Effect": "Allow",
      "Principal": "*",
      "Action": [
        "s3:GetObject"
      ],
      "Resource": "arn:aws:s3:::information-diet.blairnangle.com/*"
    },
    {
      "Sid": "AllowWriteFromAWSAccount",
      "Effect": "Allow",
      "Principal": {
        "AWS": "arn:aws:iam::${awsAccountNumber}:root"
      },
      "Action": [
        "s3:PutObject",
        "s3:PutObjectAcl",
        "s3:PutObjectTagging",
        "s3:PutObjectVersionAcl",
        "s3:PutObjectVersionTagging",
        "s3:DeleteObject",
        "s3:DeleteObjectVersion"
      ],
      "Resource": "arn:aws:s3:::information-diet.blairnangle.com/*"
    }
  ]
}
